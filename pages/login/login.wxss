page {
  background-color: #f9f9f9;
}
.userInfoWrap {
  width: 100%;
  height: 329rpx;
  overflow: hidden;
}
.userInfoWrap .unseImg {
  position: relative;
}
.userInfoWrap .unseImg image.userBg {
  width: 100%;
  height: 329rpx;
  filter: blur(15rpx);
}
.userInfoWrap .unseImg .userInfo {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  position: absolute;
  box-shadow: #ccc;
  top: 10%;
  left: 50%;
  margin: 0 auto;
  transform: translateY(-50%);
  transform: translateX(-50%);
}
.userInfoWrap .unseImg .userInfo image.userIcon {
  width: 172rpx;
  height: 172rpx;
  border-radius: 50%;
}
.userInfoWrap .unseImg .userInfo .userName {
  color: white;
  font-size: 32rpx;
  padding-top: 16rpx;
}
.userInfoWrap .unseImg .userInfo .userNameNull {
  color: black;
}
.userInfoWrap .unseImg .userInfo .userBtn {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: 0;
}
.userTools {
  margin: 10rpx auto;
  width: 726rpx;
  height: 144rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.userTools .userToolsItem {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-bottom: 10rpx
}
.userTools .userToolsItem .userToolsItemIcon {
  flex: 3;
}
.userTools .userToolsItem .userToolsItemIcon icon {
  font-size: 70rpx;
}
.userTools .userToolsItem .userToolsItemName {
  flex: 1;
  font-size: 26rpx;
}
.userSundry {
  width: 726rpx;
  height: 520rpx;
  background-color: #fff;
  margin: 0 auto;
}
.userSundry .userSundryItem {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ccc;
  height: 100rpx;
}
.userSundry .userSundryItem .userSundryItemLeft {
  display: flex;
  justify-content: center;
  align-items: center;
}
.userSundry .userSundryItem .userSundryItemLeft .userSundryItemIcon {
  flex: 1;
}
.userSundry .userSundryItem .userSundryItemLeft .userSundryItemIcon icon.iconfont.icon-tuihuotuikuan_dianpu {
  font-size: 60rpx;
  padding-left: 40rpx;
}
.userSundry .userSundryItem .userSundryItemLeft .userToolsItemName {
  padding-left: 25rpx;
  font-size: 26rpx;
}
.userSundry .userSundryItem .userSundryItemRight {
  font-style: 26rpx;
  padding-right: 46rpx;
  color: #626262;
}
