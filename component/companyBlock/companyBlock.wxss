.infoListItem {
  margin: 10rpx 12rpx;
  padding: 5rpx 0;
  background-color: #fff;
}
.infoListItem .userInfo {
  display: flex;
  align-items: center;
}
.infoListItem .userInfo .userIcon {
  width: 65rpx;
  height: 65rpx;
  border-radius: 50%;
}
.infoListItem .userInfo .userInfoRight {
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 20rpx;
}
.infoListItem .userInfo .userInfoRight .userName {
  font-size: 28rpx;
}
.infoListItem .userInfo .userInfoRight .itemTime {
  font-size: 23rpx;
}
.infoListItem .title {
  text-align: left;
  padding: 3rpx 5rpx;
}
.infoListItem .title p {
  font-size: 33rpx;
  color: #030303;
  font-weight: 500;
}
.infoListItem .content {
  padding: 3rpx 5rpx;
}
.infoListItem .content .contentDetail {
  text-align: left;
  font-size: 28rpx;
  color: #4e5459;
}
.infoListItem .content .more {
  font-size: 24rpx;
  text-align: right;
  color: #ff890ae0;
}
.infoListItem .content .conpanyPic {
  overflow: hidden;
  flex-wrap: wrap;
  display: flex;
}
.infoListItem .content .conpanyPic .conpanyPicItem {
  width: 33.33%;
  height: 15vh;
  overflow: hidden;
}
.infoListItem .content .conpanyPic .conpanyPicItem image {
  height: 100%;
  box-sizing: border-box;
  width: 100%;
  object-fit: cover;
  padding: 5px;
}
.infoListItem .itemTime {
  font-size: 25rpx;
  color: #808080;
}
.infoListItem .handle {
  height: 35rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 25rpx !important;
  color: #a4a5a9;
}
.infoListItem .handle .handleLeft {
  flex: 1;
  display: flex;
  align-items: center;
}
.infoListItem .handle .handleLeft .handleItem {
  padding-left: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.infoListItem .handle .handleLeft .handleItemNum {
  padding-left: 15rpx;
}
.infoListItem .handle .handleLeft .iconfont {
  font-size: 27rpx !important;
  padding-bottom: 17rpx;
}
.infoListItem .handle .handleRight .handleItem {
  display: flex;
  justify-content: center;
  align-items: center;
}
.infoListItem .handle .handleRight .handleItemNum {
  padding-left: 15rpx;
}
.infoListItem .handle .handleRight .iconfont {
  font-size: 27rpx !important;
  padding-bottom: 17rpx;
}
.showTwo {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
